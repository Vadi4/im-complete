extends template.pug

block title
	title main
	- var page_class = 'p-main';

block content
	.l-container
		.row.flex-center
			.col-6
				section.section
					+b('span').title-1 Basic autocomplete
					+b('').im-complete.nope-js-autocomplete
						+e('').input-wrap
							+e('input')(type="text" data-input="true").input
							+e('a')(href='#' data-open="true").btn-open
						+e('')(data-list-container="true").result-list
							+e('a')(href='#' data-list-item="true").result-item
								span(data-item-value="true") Asp
							+e('a')(href='#' data-list-item="true").result-item 
								span(data-item-value="true") Basic
							+e('a')(href='#' data-list-item="true").result-item 
								span(data-item-value="true") C++
							+e('a')(href='#' data-list-item="true").result-item 
								span(data-item-value="true") Erlang
							+e('a')(href='#' data-list-item="true").result-item 
								span(data-item-value="true") Haskell
							+e('a')(href='#' data-list-item="true").result-item 
								span(data-item-value="true") JavaScript
							+e('a')(href='#' data-list-item="true").result-item 
								span(data-item-value="true") TypeScript
							+e('a')(href='#' data-list-item="true").result-item 
								span(data-item-value="true") Lisp
							+e('a')(href='#' data-list-item="true").result-item 
								span(data-item-value="true") PHP
				section.section
					+b('span').title-1 Dynamic fetch autocomplete
					+b('')(data-response-path="/response.json").im-complete.nope-js-autocomplete-dynamic-ajax
						+e('').input-wrap
							+e('input')(type="text" data-input="true").input
							+e('a')(href='#' data-open="true").btn-open
						+e('')(data-list-container="true").result-list
				section.section
					+b('span').title-1 Ajax autocomplete
					+b('')(data-response-path="/response.json").im-complete.nope-js-autocomplete-ajax
						+e('').input-wrap
							+e('input')(type="text" data-input="true").input
							+e('a')(href='#' data-open="true").btn-open
						+e('')(data-list-container="true").result-list




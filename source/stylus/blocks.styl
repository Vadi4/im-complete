
.b-im-complete
	&__input
		border-radius 4px
		border 2px solid rgba(0, 0, 0, .5)
		padding 0 24px
		font-size 24px
	&__result-list
		border 2px solid rgba(0, 0, 0, .5)
		border-top 0
		border-radius 0 0 4px 4px
	&__result-item
		padding 12px 24px
		text-decoration none
		color black
		transition .25s background-color
		@media $working_hover
			&:hover
				background-color #fffdd1	

.l-footer
	width 100%
	margin-top auto
	margin-bottom 0